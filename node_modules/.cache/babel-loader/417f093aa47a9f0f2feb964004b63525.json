AAAA,IAAI,EAAE,OAFR;AAGEC,IAAAA,SAAS,EAAE;AAAA,aACT,OAAO,mBAAP,CADS;AAAA;AAHb,GA/BM,EAqCN;AACEF,IAAAA,IAAI,EAAE,WADR;AAEEC,IAAAA,IAAI,EAAE,UAFR;AAGEC,IAAAA,SAAS,EAAE;AAAA,aACT,OAAO,sBAAP,CADS;AAAA;AAHb,GArCM,EA2CN;AACEF,IAAAA,IAAI,EAAE,SADR;AAEEC,IAAAA,IAAI,EAAE,QAFR;AAGEC,IAAAA,SAAS,EAAE;AAAA,aACT,OAAO,oBAAP,CADS;AAAA;AAHb,GA3CM,EAiDN;AACEF,IAAAA,IAAI,EAAE,OADR;AAEEC,IAAAA,IAAI,EAAE,MAFR;AAGEC,IAAAA,SAAS,EAAE;AAAA,aACT,OAAO,kBAAP,CADS;AAAA;AAHb,GAjDM,EAuDN;AACEF,IAAAA,IAAI,EAAE,KADR;AAEEC,IAAAA,IAAI,EAAE,IAFR;AAGEC,IAAAA,SAAS,EAAE;AAAA,aACT,OAAO,sBAAP,CADS;AAAA;AAHb,GAvDM,EA6DN;AACEF,IAAAA,IAAI,EAAE,WADR;AAEEC,IAAAA,IAAI,EAAE,UAFR;AAGEC,IAAAA,SAAS,EAAE;AAAA,aACT,OAAO,sBAAP,CADS;AAAA;AAHb,GA7DM,EAmEN;AACEF,IAAAA,IAAI,EAAE,UADR;AAEEC,IAAAA,IAAI,EAAE,SAFR;AAGEC,IAAAA,SAAS,EAAE;AAAA,aACT,OAAO,qBAAP,CADS;AAAA;AAHb,GAnEM;AAHiB,CAAX,CAAhB,C,CAiFA;;AACAT,MAAM,CAACa,UAAP,CAAkB,UAACC,EAAD,EAAKC,IAAL,EAAWC,IAAX,EAAoB;AAEpC,MAAML,YAAY,GAAGG,EAAE,CAACG,OAAH,CAAWC,IAAX,CAAgB,UAAAC,CAAC;AAAA,WAAIA,CAAC,CAACT,IAAF,CAAOC,YAAX;AAAA,GAAjB,CAArB;AACA,MAAMS,WAAW,GAAGtB,EAAE,CAACuB,IAAH,GAAUD,WAA9B,CAHoC,CAKpC;;AACA,MAAIT,YAAY,IAAI,CAACS,WAArB,EAAkC;AAC9BJ,IAAAA,IAAI,CAAC,GAAD,CAAJ;AACH,GAFD,MAEO,IAAIL,YAAY,IAAIS,WAApB,EAAiC;AACpCJ,IAAAA,IAAI;AACP,GAFM,MAEA;AACHA,IAAAA,IAAI;AACP;AACF,CAbD;AAeA,eAAehB,MAAf","sourcesContent":["import Vue from \"vue\";\nimport Router from \"vue-router\";\n\nimport Home from \"./views/Home.vue\";\nimport User from \"./views/User.vue\";\n\nimport Profile from \"./views/Profile.vue\";\nimport Orders from \"./views/Orders.vue\";\nimport MProducts from \"./views/MProducts.vue\";\n\n// import WordList from \"./views/WordList.vue\"\n// import WordMdf from \"./views/WordMdf.vue\"\n// import WordAdd from \"./views/WordAdd.vue\"\n\nimport {fb} from './firebase'\n\nVue.use(Router);\n\nconst router =  new Router({\n  mode: \"history\",\n  base: process.env.BASE_URL,\n  routes: [\n    \n    {\n      path: \"/\",\n      name: \"home\",\n      component: Home\n    },\n    {\n      path: \"/user\",\n      name: \"user\",\n      component: User,\n      meta: { requiresAuth: true }, // 登入前須經過驗證\n      // User.vue的子分頁\n      children:[\n        {\n          path: \"profile\",\n          name: \"profile\",\n          component: Profile\n        },\n        {\n          path: \"orders\",\n          name: \"orders\",\n          component: Orders\n        },\n        {\n          path: \"Mproducts\",\n          name: \"Mproducts\",\n          component: MProducts\n        },\n      ]\n    },\n    {\n      path: \"/about\",\n      name: \"about\",\n      component: () =>\n        import(\"./views/About.vue\")\n    },\n    {\n      path: \"/products\",\n      name: \"products\",\n      component: () =>\n        import(\"./views/Products.vue\")\n    },\n    {\n      path: \"/stores\",\n      name: \"stores\",\n      component: () =>\n        import(\"./views/Stores.vue\")\n    },\n    {\n      path: \"/news\",\n      name: \"news\",\n      component: () =>\n        import(\"./views/News.vue\")\n    },\n    {\n      path: \"/n2\",\n      name: \"n2\",\n      component: () =>\n        import(\"./views/WordAdd2.vue\")\n    },\n    {\n      path: \"/WordList\",\n      name: \"WordList\",\n      component: () =>\n        import(\"./views/WordList.vue\")\n    }, \n    {\n      path: \"/WordAdd\",\n      name: \"WordAdd\",\n      component: () =>\n        import(\"./views/WordAdd.vue\")\n    },\n\n\n  ]\n});\n\n//需經過驗證才可登入\nrouter.beforeEach((to, from, next) => {\n\n  const requiresAuth = to.matched.some(x => x.meta.requiresAuth)\n  const currentUser = fb.auth().currentUser\n\n  // 未經過驗證則到首頁\n  if (requiresAuth && !currentUser) {\n      next('/')\n  } else if (requiresAuth && currentUser) {\n      next()\n  } else {\n      next()\n  }\n})\n\nexport default router;"]}]}